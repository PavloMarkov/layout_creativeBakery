ont-weight: 500;\n  font-size: 16px;\n  line-height: 100%;\n  color: #f9512e;\n  letter-spacing: 0.02em;\n  text-transform: uppercase;\n  text-decoration: none;\n\n  &::before {\n    content: \"\";\n    display: block;\n    width: 20px;\n    height: 20px;\n    margin-right: 6px;\n    background-image: url(../images/button_cookie_norm.svg);\n  }\n\n  &:hover {\n    background-color: #f9512e;\n    color: #fff;\n\n    &::before {\n      content: \"\";\n      display: block;\n      width: 20px;\n      height: 20px;\n      margin-right: 6px;\n      background-image: url(../images/button_cookie_hover.svg);\n    }\n  }\n\n  &:focus {\n    background-color: #b3341a;\n    color: #fff;\n    border-color: #b3341a;\n\n    &::before {\n      content: \"\";\n      display: block;\n      width: 20px;\n      height: 20px;\n      margin-right: 6px;\n      background-image: url(../images/button_cookie_hover.svg);\n    }\n  }\n}\n\n.menu {\n  position: fixed;\n  background-color: #eef7ff;\n  height: 100vh;\n  left: 0;\n  right: 0;\n  top: 0;\n  transform: translateX(-100%);\n\n  &:target {\n    transform: translateX(0);\n  }\n\n  &__list {\n    position: absolute;\n    top: 50%;\n    left: 50%;\n    transform: translate(-50%, -50%);\n  }\n}\n\n.list {\n\n  @include onMobile {\n    display: none;\n  }\n\n  @include onDesktop {\n    display: flex;\n  }\n\n  margin: 0;\n  padding: 0;\n  list-style-type: none;\n\n  &--menu {\n    display: grid;\n    grid-template-columns: 1fr;\n    justify-content: center;\n    align-items: center;\n    gap: 24px;\n  }\n  &__link {\n    display: block;\n    text-decoration: none;\n    margin-left: 64px;\n    text-align: center;\n    font-size: 16px;\n    line-height: 150%;\n    letter-spacing: -0.01em;\n    color: #1c1a26;\n\n    &--menu {\n      margin-left: 0;\n    }\n\n    &--last {\n      color: #f9512e;\n      height: 100%;\n      display: flex;\n      justify-content: center;\n      align-items: center;\n      letter-spacing: 0.02em;\n      text-transform: uppercase;\n      font-weight: 500;\n      line-height: 100%;\n\n      &::before {\n        content: \"\";\n        display: block;\n        width: 18px;\n        height: 18px;\n        margin-right: 6px;\n        background-image: url(../images/red_phone.png);\n        background-repeat: no-repeat;\n        background-position: center;\n        background-size: cover;\n      }\n    }\n  }\n}\n","styles/utils/_vars.scss":"$c-gray: #eef7ff;\n","styles/utils/_mixins.scss":"@mixin hover($_property, $_toValue) {\n  transition: #{$_property} 0.3s;\n  &:hover {\n    #{$_property}: $_toValue;\n  }\n}\n\n@mixin onMobile {\n  @media (min-width: 1px) {\n    @content;\n  }\n}\n\n@mixin onDesktop {\n  @media (min-width: 1020px) {\n    @content;\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"8fdf2fcf9a9baa0e662da30e0cdd38f0","cacheData":{"env":{}}}